class Solution {
    static boolean isPrime(int n) {
         int sum=0;
         if(n==1)
         {
             return false;
         }
         else if(n==2)
         {
             return true;
         }
         else
         {
         for(int i=2;i<n;i++)
         {
             if(n%i==0)
             {
                 return false;
             }
         }
         }
         return true;
    }
}
